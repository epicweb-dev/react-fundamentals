{
  "name": "@kentcdodds/react-fundamentals",
  "version": "1.0.0",
  "description": "The material for learning React fundamentals",
  "title": "React Fundamentals âš›",
  "keywords": [],
  "homepage": "http://react-fundamentals.netlify.app/",
  "license": "GPL-3.0-only",
  "main": "src/index.js",
  "engines": {
    "node": "^10.13 || 12 || 14 || 15",
    "npm": ">=6"
  },
  "dependencies": {
    "@kentcdodds/react-workshop-app": "^2.21.0",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "chalk": "^4.1.0",
    "codegen.macro": "^4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "husky": "^4.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --watchAll=false",
    "test:exercises": "npm run test -- testing.*exercises\\/ --onlyChanged",
    "setup": "node setup",
    "lint": "eslint .",
    "typecheck": "tsc",
    "validate": "npm-run-all --parallel build test:coverage lint typecheck"
  },
  "husky": {
    "hooks": {
      "pre-commit": "node ./scripts/pre-commit",
      "pre-push": "node ./scripts/pre-push"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/final/**/*.tsx"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kentcdodds/react-fundamentals.git"
  },
  "author": "Kent C. Dodds <me@kentcdodds.com> (https://kentcdodds.com/)",
  "bugs": {
    "url": "https://github.com/kentcdodds/react-fundamentals/issues"
  }
}
